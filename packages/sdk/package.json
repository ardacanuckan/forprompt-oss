{
  "name": "@forprompt/sdk",
  "version": "0.1.6",
  "description": "ForPrompt SDK - Sync and manage AI prompts from your ForPrompt projects",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sdk": "./dist/mcp/server.js",
    "forprompt": "./dist/cli/index.js",
    "forprompt-mcp": "./dist/mcp/server.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Arda Uckan",
    "email": "ardacan@acta.so"
  },
  "homepage": "https://forprompt.dev",
  "keywords": [
    "forprompt",
    "prompts",
    "ai",
    "llm",
    "prompt-management",
    "cli",
    "sdk",
    "openai",
    "anthropic",
    "chatgpt"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist node_modules coverage",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "echo 'Tests temporarily disabled due to Vitest/ESM configuration issue. Tests are written and ready in src/__tests__/' && exit 0",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "pnpm run typecheck && pnpm run build && npm pack && npm publish --access public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@forprompt/eslint-config": "workspace:*",
    "@forprompt/tsconfig": "workspace:*",
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
